<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="cacheEnabled"     value="true"/>
        <setting name="useGeneratedKeys" value="true"/>
        <setting name="defaultExecutorType" value="REUSE"/>
    </settings>

    <typeAliases>
        <typeAlias type="io.aetherit.kfashion.ws.model.KfashionUser" alias="KfashionUser" />
        <typeAlias type="io.aetherit.kfashion.ws.model.KfashionBasic" alias="KfashionBasic" />
        <typeAlias type="io.aetherit.kfashion.ws.model.KfashionImageLabeling" alias="KfashionImageLabeling" />
        <typeAlias type="io.aetherit.kfashion.ws.model.KfashionImageLabelingLocation" alias="KfashionImageLabelingLocation" />
        <typeAlias type="io.aetherit.kfashion.ws.model.KfashionInspectionStatus" alias="KfashionInspectionStatus" />
        <typeAlias type="io.aetherit.kfashion.ws.model.KfashionProfessional" alias="KfashionProfessional" />
        <typeAlias type="io.aetherit.kfashion.ws.model.KfashionSimpleUser" alias="KfashionSimpleUser" />
        <typeAlias type="io.aetherit.kfashion.ws.model.KfashionStudies" alias="KfashionStudies" />
        <typeAlias type="io.aetherit.kfashion.ws.model.support.KfashionUserType" alias="KfashionUserType" />
    </typeAliases>

    <typeHandlers>
        <typeHandler handler="io.aetherit.kfashion.ws.repository.typehandler.BooleanYnTypeHandler" javaType="Boolean" />
    </typeHandlers>

    <mappers>
        <mapper resource="mybatis/mapper/mariadb/UserMapper.xml" />
        <mapper resource="mybatis/mapper/mariadb/KfashionBasicMapper.xml" />
        <mapper resource="mybatis/mapper/mariadb/KfashionImageLabelingLocationMapper.xml" />
        <mapper resource="mybatis/mapper/mariadb/KfashionImageLabelingMapper.xml" />
        <mapper resource="mybatis/mapper/mariadb/KfashionInspectionStatusMapper.xml" />
        <mapper resource="mybatis/mapper/mariadb/KfashionInspectionStatusMapper.xml" />
        <mapper resource="mybatis/mapper/mariadb/KfashionStudiesMapper.xml" />
    </mappers>
</configuration>