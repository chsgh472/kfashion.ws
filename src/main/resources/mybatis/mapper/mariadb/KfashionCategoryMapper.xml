<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.aetherit.kfashion.ws.repository.mapper.KfashionCategoryMapper">
    <resultMap id="kfashionCategoryResultMap" type="KfashionCategory">
        <id property="category_no"                       column="category_no" />
        <result property="category_male"             column="category_male" />
        <result property="category_female"                 column="category_female" />
        <result property="category_unisex"                 column="category_unisex" />
    </resultMap>

    <select id="categoryList" resultType="KfashionCategory">
        select category_male from kfashion_category
    </select>

</mapper>
