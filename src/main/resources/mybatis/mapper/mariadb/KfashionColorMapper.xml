<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.aetherit.kfashion.ws.repository.mapper.KfashionColorMapper">
    <resultMap id="kfashionColorResultMap" type="KfashionColor">
        <id property="category_no"                       column="category_no" />
        <result property="category_male"             column="category_male" />
        <result property="category_female"                 column="category_female" />
        <result property="category_unisex"                 column="category_unisex" />
    </resultMap>

    <select id="colorList" resultType="KfashionColor">
        select
        color_name,
        color_hexcode,
        color_rgb
        from kfashion_color
    </select>
</mapper>
